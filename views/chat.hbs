<div class="chat">
    <div id="user-data" class="container">
        <div class="row">
            <div class="col-1">Name: </div>
            <div class="col" id="name">{{userData.name}}</div> ||
            <div class="col-1">Room: </div>
            <div class="col" id="room">{{userData.room}}</div>
        </div>
    </div>
    <div class="messages container" id="list">
        <div id="inner"></div>
    </div>
    <div class="input">
        <div class="container">
            <form action="" id="msg-form">
                <div class="input-group row">
                    <input id="new-msg" type="text" class="form-control col" autocomplete="off" >
                    <div class="input-group-append">
                        <button class="btn btn-dark">Send</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>